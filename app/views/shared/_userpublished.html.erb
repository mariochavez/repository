<% if user_signed_in? %>
	<% if current_user.publications.count >= 1 %>
	<div id="user-published">
    	<h3><span>Mis publicaciones</span> <%= link_to 'Ver todas', mypublications_path %></h3>
    	<ul>
		<% @mypublications.each do |mp| %>
        	<li><a title="<%= mp.title %>" href="<%= publication_path mp %>"><%= mp.title %></a></li>
		<% end %>
    	</ul>
	</div><!--end user-published-->
	<% end %>

<% end %>